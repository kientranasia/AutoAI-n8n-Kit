# Minimal .env.example for AutoAI-n8n-Kit

# PostgreSQL
POSTGRES_USER=autoai
POSTGRES_PASSWORD=autoai_pass
POSTGRES_DB=autoai_db

# n8n
N8N_URL=http://localhost:5678
N8N_ENCRYPTION_KEY=your_n8n_encryption_key
N8N_USER_MANAGEMENT_JWT_SECRET=your_n8n_jwt_secret

# Cloudflare Tunnel
CLOUDFLARE_TUNNEL_TOKEN=your_cloudflare_tunnel_token